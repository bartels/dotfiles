[user]
	name = Eric Bartels
	email = ebartels@gmail.com
[core]
	excludesfile = ~/.gitignore
[push]
	default = simple
[alias]
	co = checkout
	st = status
	br = branch
        lg = log --graph --all --decorate --abbrev-commit
[color]
	diff = auto
	ui = true
[color "status"]
	added = green
	changed = yellow
	untracked = white
[color "branch"]
	current = green bold
	local = normal
	remote = white
[heroku]
	account = personal
